<div class="media-heading">
    Crearon una nueva publicacion en <%= Category.find(notification.target.category_id).name %>
</div>
<div class="media-content">
  <%= notification.actor.username %> creo 
  <%=link_to(notification.target.title ,main_app.post_path(notification.target.id), onclick: "render(this);")%>
</div>
<script>
  function render(obj) {
      var href = obj.href
      console.log(href)
      if (window.location !== window.top.location) {
          window.top.location.href = href
      }
  };
</script>